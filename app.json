{
  "expo": {
    "name": "easymoney",
    "slug": "easymoney-app",
    "description": "React Native APP",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#343536"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.rafanonato.easymoney",
      "supportsTablet": true
    },
    "android": {
      "package": "com.rafanonato.easymoney",
      "versionCode": 1
    }
  }
}
